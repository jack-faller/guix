seat seat0 xcursor_theme Quintom_Ink 12
exec dunst &
exec gammastep -l 50.721680:-1.878530 &
include ~/.config/sway/configs/base-config
exec ~/.guix-home/profile/libexec/xdg-desktop-portal-wlr
exec ~/.config/startup-programs.sh
